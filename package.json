{
  "name": "restau-apis",
  "version": "1.0.0",
  "description": "a backend food delivery api",
  "main": "app.js",
  "scripts": {
    "test:script": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/bin/dev",
    "dev": "babel-watch src/server.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "prod": "npm run build && node src/bin/prod",
    "lint": "eslint",
    "migrate": "babel-node src/db/migrations/migrations.js",
    "migrate:test": "cross-env NODE_ENV=test npm run migrate",
    "test": "npm run migrate:test && cross-env NODE_ENV=test nyc cross-env mocha --require babel-core/register src/tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArnauldAnguh/Restau-apis.git"
  },
  "keywords": [
    "node",
    "api",
    "restful"
  ],
  "author": "Arnauld Anguh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArnauldAnguh/Restau-apis/issues"
  },
  "homepage": "https://github.com/ArnauldAnguh/Restau-apis#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "*",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pg": "*",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-watch": "^7.0.0",
    "chai": "^*",
    "chai-http": "*",
    "coveralls": "*",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "nyc": "^13.0.1"
  }
}
